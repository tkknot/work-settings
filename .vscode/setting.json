{
  "files.autoSave": "afterDelay",
  "editor.tabSize": 2,
  "editor.fontSize": 16,
  "editor.wordWrap": "off",
  "editor.fontFamily": "Ricty Diminished",
  "editor.renderWhitespace": "all",
  "breadcrumbs.enabled": false,

  "workbench.editor.showTabs": "multiple",
  "workbench.iconTheme": "material-icon-theme",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "editor.detectIndentation": false,
  "editor.smoothScrolling": true,
  "files.autoGuessEncoding": true,
  "editor.semanticTokenColorCustomizations": null,
  "workbench.startupEditor": "none",
  "workbench.colorCustomizations": {
    "terminal.foreground": "#A5A2A2",
    "terminalCursor.background": "#A5A2A2",
    "terminalCursor.foreground": "#A5A2A2",
    "terminal.ansiBlack": "#090300",
    "terminal.ansiBlue": "#01A0E4",
    "terminal.ansiBrightBlack": "#5C5855",
    "terminal.ansiBrightBlue": "#01A0E4",
    "terminal.ansiBrightCyan": "#B5E4F4",
    "terminal.ansiBrightGreen": "#01A252",
    "terminal.ansiBrightMagenta": "#A16A94",
    "terminal.ansiBrightRed": "#DB2D20",
    "terminal.ansiBrightWhite": "#F7F7F7",
    "terminal.ansiBrightYellow": "#FDED02",
    "terminal.ansiCyan": "#B5E4F4",
    "terminal.ansiGreen": "#01A252",
    "terminal.ansiMagenta": "#A16A94",
    "terminal.ansiRed": "#DB2D20",
    "terminal.ansiWhite": "#A5A2A2",
    "terminal.ansiYellow": "#FDED02"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "git.terminalAuthentication": false,
  "security.allowedUNCHosts": ["wsl.localhost"],
  "git.confirmSync": false,
  "editor.fontLigatures": false,
  "terminal.integrated.fontFamily": "Ricty Diminished",
  "workbench.settings.applyToAllProfiles": ["editor.formatOnSave"],
  "dev.containers.executeInWSL": true,
  "workbench.colorTheme": "Visual Studio Dark",
  "remote.autoForwardPortsSource": "hybrid",
  "codeium.enableConfig": {
    "*": true
  },
  "vim.leader": "<Space>",
  "vim.vimrc.enable": true,
  "vim.easymotion": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.ignorecase": true,
  "vim.incsearch": true,
  "vim.wrapscan": true,
  "vim.sneak": true,
  "vim.autoindent": true,
  "vim.smartRelativeLine": true,
  "vim.showcmd": true,
  "vim.useSystemClipboard": true,
  "vim.vimrc.path": "$HOME/.vimrc",
  "vim.statusBarColors.insert": "#BF616A",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<space>", "v"],
      "commands": [":vsplit"]
    },
    {
      "before": ["<space>", "h"],
      "commands": [":bprevious"]
    },
    {
      "before": ["<space>", "l"],
      "commands": [":bnext"]
    },
    {
      "before": ["H"],
      "after": ["0"]
    },
    {
      "before": ["L"],
      "after": ["$"]
    },
    {
      "before": ["<space>", "w"],
      "commands": [":w!"]
    },
    {
      "before": ["<space>", "q"],
      "commands": [":q!"]
    },
    {
      "before": ["<space>", "x"],
      "commands": ["x!"]
    },
    {
      "before": ["<space>", "c"],
      "commands": ["editor.action.commentLine"]
    },
    {
      "before": ["/"],
      "commands": ["actions.find"]
    }
  ],
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["Esc"]
    },
    {
      "before": ["<C-i>"],
      "after": ["codeium.openCodeiumCommand"]
    }
  ],
  "diffEditor.ignoreTrimWhitespace": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "github.copilot.editor.enableAutoCompletions": true
}

